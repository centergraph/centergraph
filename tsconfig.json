{
  "compilerOptions": {
    "esModuleInterop": true,
    "noEmit": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["ESNext"],
    "allowImportingTsExtensions": true,
    "paths": {
      "@centergraph/shared/*": ["./packages/shared/*"],
      "@centergraph/shacl-renderer/*": ["./packages/shacl-renderer/*"],
      "@centergraph/turtle-sync/*": ["./packages/turtle-sync/*"],
      "@centergraph/sdk": ["./packages/sdk/lib/CenterGraph.ts"],
      "@centergraph/sdk/*": ["./packages/sdk/*"]
    }
  },
  "ts-node": {
    "esm": true
  }
}
